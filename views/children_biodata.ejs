<%- include("partials/header"); -%>


 <div class="templateBody-container">

  <div class="templateBody-topBreadcrumb">
   <div class="back-to-homepage">
    <a href="/" class="home-link"><i class="fa fa-home"></i>Back to Homepage</a>
    <span class="pageName-span">Children Treatment Path- Biodata</span>
   </div>
  </div>


  <div class="templateBody-sideBreadcrumb">
   <h2 class="parentPage-link">
    <a href="#">Children Health</a>
   </h2>
   <ul class="descendantsPage-links">
    <li class="currentPage"><a href="#">Biodata Page</a>
    </li>
    <li><a href="/children_preventive">Preventive Children Health</a></li>
   </ul>
  </div>
  <br><br>

  <div class="templateBody-main">

   <form action="/children_biodata" method="post" id="medicalForm">

      <p class="fireBrick">Please click the age range that applies</p>
    
    <input type="radio" name="age" value="i"  checked/>0 - 11months
    <input type="radio" name="age" value="u"  />1 - 5yrs
    <input type="radio" name="age" value="c" />6 - 12yrs
    <input type="radio" name="age" id="t" value="t" />13 - 19yrs
    <br /><br />
    
    <p class="fireBrick">Please click applicable gender</p>
    <input type="radio" name="gender" value="m" checked/>Male
    <input type="radio" name="gender" id="f" value="m" />Female
   <br><br>

     <input type="checkbox" onclick="changeValue()" required><b style="color: firebrick;">click when done</b>
    <br><br>  
    
    <br><br><br>
    <button type="submit" name="button" class="next-btn">Next</button>

   </form>




   <script>

  function changeValue() {
      if (document.getElementById("t").checked == true) {
        document.getElementById("f").value = "f"
      } else {
        document.getElementById("f").value = "m"
      }

    }
   </script>
  </div>
 </div>




