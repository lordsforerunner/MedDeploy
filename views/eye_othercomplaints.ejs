<%- include("partials/header"); -%>


 <div class="templateBody-container">

  <div class="templateBody-topBreadcrumb">
   <div class="back-to-homepage">
    <a href="/" class="home-link"><i class="fa fa-home"></i>Back to Homepage</a>
    <span class="pageName-span">Eye Treatment Path- Othercomplaints</span>
   </div>
  </div>
  <br><br>
  <div class="templateBody-main">

   <h1>Other Complaints</h1>

   <p class="fireBrick">Click the applicable button below to proceed</p>
   <p class="fireBrick">Note => If you have only one complaint, click appropriately and <a href="#contact">scroll down</a> to move to the next page</p>


   <form action="/eye_othercomplaints" method="post" id="medicalForm">
    <input type="radio" name="trying" id="no" value="no" onclick="stallBack()"><b>I have only one
     complaint</b>&nbsp;&nbsp;&nbsp;&nbsp;
    <input type="radio" name="trying" id="yes" value="yes" onclick="symptomChecker()"><b>I have other complaints
    </b>
    <br><br>

    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="OneEyeWeakVision" disabled/><b>Weak vision</b> in <u>one
     eye</u><br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="Asymmetry" disabled/>Lack
    of symmetry when you look at your eyes in the mirror<br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="OneEyeUnconMovt" disabled/>Uncontrollable movement in <u>one
     eye</u><br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="ColorDef" disabled/>Difficulty with differentiating
    colors<br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="RblurredVision" disabled/><b>Recent onset(few hours or
     days)</b> of
    <u>blurred vision</u><br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="CblurredVision" disabled/><b>Longterm onset(more than two
     weeks)</b>
    of <u>blurred vision</u><br />
    
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="RbothEyesPain" disabled/><b>Recent onset(few hours or
     days)</b>
    of both <u>eyes pain</u><br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="CbothEyesPain" disabled/><b>Longterm onset(more than two
     weeks)</b> of
    both <u>eyes pain</u> <br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="OneEyePain" disabled/>Pain in
    <b>one eye</b><br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="EyesRedness" disabled/>Eyes
    redness<br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="LightSensitivity" disabled/>Light sensitivity<br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="WeakVisionClose" disabled/>Weak vision for objects that are
    <u>close</u><br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="RpepperySensatn" disabled/><b>Recent onset(few hours or
     days)</b> of
    burning or peppery sensation
    in the eyes<br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="CpepperySensatn" disabled/><b>Longterm onset(more than two
     weeks)</b>
    of burning or peppery
    sensation in the eyes<br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="ExTearing" disabled/>Excessive <b>tearing</b> from the eyes<br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="HaloVision" disabled/>You
    see <b>bright circles or rings around a light source</b><br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="SpottyVision" disabled/><b>Sudden</b> appearance or increase in
    seeing <b>spots
     or flashes of light</b> in
    your vision field<br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="PoorNightVision" disabled/>Difficulty with seeing clearly <u>at
     night</u><br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="ReyesItching" disabled/><b>Recent onset(few hours or
     days)</b>
    of <u>eyes itching</u><br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="CeyesItching" disabled/><b>Longterm onset(more than two
     weeks)</b>
    of recurrent <u>eyes itching</u><br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="ParticlesInEye" disabled/>Feeling of particles in the
    eyes<br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="EyesDischarge" disabled/><b>Eyes discharge</b><br>
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="GeneralizedHeadache" disabled/><u>Generalized</u>
    <b>headaches</b><br />
    <input type="checkbox" name="othersymptoms" id="othersymptoms" value="OneSidedHeadache" disabled/><u>One sided</u>
    <b>headaches</b>

    <br><br><br>
    <h3 class="fireBrick" id="eureka"></h3><br>
    <input type="checkbox" name="checker" id="checker" onclick="detective()" required><b style="color: firebrick;">click when done</b>
    <br><br>

    <br><br>
    <button type="submit" class="next-btn" name="submit" id="submitbtn" disabled>Next</button>
    <br><br>
    <button type="reset" onClick="window.location.reload();">Reset</button>

   </form>


   <script>
    var gotIt = document.getElementsByName("othersymptoms");
    var yes = document.getElementById("yes");
    var no = document.getElementById("no");

    function detective() {
     var myReka = [];
     document.getElementById('eureka').innerHTML = "";
     if (no.checked) {
      document.getElementById("submitbtn").removeAttribute("disabled");

     } else {
      for (i = 0; i < othersymptoms.length; i++) {
       myReka.push("one");
       if (othersymptoms[i].checked) {
        document.getElementById("submitbtn").removeAttribute("disabled");

       }
       if (othersymptoms.length === myReka.length && document.getElementById("submitbtn").disabled) {
        document.getElementById('eureka').innerHTML = "Please check your other complaint(s); if not found, contact us (stating your age, gender and complaints) for possible future accommodation and any other action deemed necessary. Thank you";
        document.getElementById("checker").checked = false
       }
      }
     }


    }

    function symptomChecker() {
     for (c = 0; c < gotIt.length; c++) {
      if (gotIt[c].value !== "<%= Mainsymptom  %>" && yes.checked) {
       gotIt[c].removeAttribute("disabled");
      }
     }
    }

    function stallBack() {
     for (c = 0; c < gotIt.length; c++) {
      gotIt[c].disabled = true;
     }
    }

   </script>

  </div>
 </div>


